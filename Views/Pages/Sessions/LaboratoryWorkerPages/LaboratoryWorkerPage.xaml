<Page x:Class="MedicalLaboratoryNumber20App.Views.Pages.Sessions.LaboratoryWorkerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MedicalLaboratoryNumber20App.Views.Pages.Sessions"
      mc:Ignorable="d"
      d:DesignHeight="450"
      d:DesignWidth="800"
      Title="Страница лаборанта">
    <Grid>
        <StackPanel HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <Button Content="Биоматериалы из поликлиник города"
                    Width="auto"
                    ToolTip="Открыть страницу для принятия биоматериалов"
                    Click="NavigateToBiomaterials"></Button>
            <Button Content="Сформировать отчёты"
                    Width="auto"
                    ToolTip="Открыть страницу формирования отчётов"
                    Click="GoToReportCreationPage"></Button>
        </StackPanel>
    </Grid>
</Page>
